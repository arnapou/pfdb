{
    "name": "arnapou/pfdb",
    "description": "Arnapou pfdb",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Buathier",
            "email": "arnaud@arnapou.net",
            "homepage": "https://arnapou.net/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    },
    "require": {
        "php": ">=8.2",
        "symfony/yaml": "^5.4",
        "symfony/lock": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": ">=3.13",
        "phpstan/phpstan": ">=1.9",
        "phpunit/phpunit": ">=9.5",
        "vimeo/psalm": ">=5.1"
    },
    "autoload": {
        "psr-4": {
            "Arnapou\\PFDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnapou\\PFDB\\Tests\\": "tests/"
        }
    }
}
