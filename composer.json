{
    "name": "arnapou/pfdb",
    "description": "A flat file storage with query features",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Buathier",
            "email": "arnaud@arnapou.net",
            "homepage": "https://arnapou.net/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    },
    "require": {
        "php": "^8.2 || ^8.3",
        "symfony/yaml": "^5.4",
        "symfony/lock": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "phpunit/php-code-coverage": "^10.0",
        "vimeo/psalm": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Arnapou\\PFDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnapou\\PFDB\\Tests\\": "tests/"
        }
    }
}
